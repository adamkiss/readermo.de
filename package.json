{
  "name": "readermo.de",
  "version": "0.0.1",
  "description": "Use Reader Mode, any page, any time.",
  "author": "Adam Kiss",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@fortawesome/pro-light-svg-icons": "^5.6.3",
    "@fortawesome/pro-regular-svg-icons": "^5.6.3",
    "@fortawesome/pro-solid-svg-icons": "^5.6.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "cross-env": "^5.2.0",
    "is-url": "^1.2.4",
    "node-sass": "^4.11.0",
    "nuxt": "^2.3.4",
    "nuxt-fontawesome": "^0.4.0",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "eslint-plugin-vue": "^5.1.0",
    "nodemon": "^1.18.9",
    "xo": "^0.24.0"
  },
  "xo": {
    "plugins": [
      "vue"
    ],
    "semicolon": false,
    "rules": {
      "import/order": 0,
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "**/*.css"
          ]
        }
      ],
      "generator-star-spacing": 0,
      "capitalized-comments": 0,
      "require-yield": 0,
      "func-names": 0,
      "operator-linebreak": 0,
      "curly": [
        "error",
        "multi",
        "consistent"
      ]
    }
  }
}
